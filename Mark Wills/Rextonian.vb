#Region " Translate"


#Region " Button click / Enter pressed"

    Private Sub me_English_KeyUp(sender As Object, e As KeyEventArgs) Handles me_English.KeyUp
        If e.KeyCode = Keys.Enter Then
            TranslatePressed()
        End If
    End Sub
    Private Sub but_Translate_Click(sender As Object, e As EventArgs) Handles but_Translate.Click
        TranslatePressed()
    End Sub

#End Region
#Region " Translate Code"

    Public Function Translate(EnglishWord As String, RextonianWord As String, source As String) As String

        Dim pattern As String = String.Format("\b{0}\b", EnglishWord)
        Return RegularExpressions.Regex.Replace(source, pattern, RextonianWord, RegularExpressions.RegexOptions.IgnoreCase)

    End Function


#End Region

#Region " Dictionary"
    Private Sub but_Add_Click(sender As Object, e As EventArgs) Handles but_Add.Click
        Dim DictionaryFile As String = Application.StartupPath + "\Rextonian.dic"

        Dim DictionaryString As String() = IO.File.ReadAllLines(DictionaryFile)

        Dim Englishword As String
        Dim Rextonianword As String



        Dim xform As New AddWord
        If xform.ShowDialog = DialogResult.OK Then
            Englishword = xform.tb_English.Text
            Rextonianword = xform.tb_Rextonian.Text
            IO.File.AppendAllText(DictionaryFile, Environment.NewLine & Englishword & "=" & Rextonianword)

        End If



    End Sub




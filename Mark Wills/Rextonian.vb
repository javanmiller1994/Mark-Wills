    Public Sub ReadDictionary()
        Try
            Dim s As String = EnglishText
            Dim DictionaryFile As String = Application.StartupPath + "\Rextonian.dic"

            Dim DictionaryString As String() = IO.File.ReadAllLines(DictionaryFile)
            ' Dim DictionaryString As String = IO.File.ReadAllText(DictionaryFile)

            ' Dim Ar() As String = DictionaryString.Split(Environment.NewLine)

            For Each sr As String In DictionaryString
                ' s is the variable to process
                Dim str1 As String = sr.Substring(0, sr.IndexOf("=")).ToString
                Dim str2 As String = sr.Substring(sr.IndexOf("=") + 1).ToString


                s = Translate(str1, str2, s)
                EnglishText = s
                '     MsgBox(s)

            Next

        Catch ex As Exception

        End Try

    End Sub

#Region " Animate / Expand button"

    Dim animationSpeed As Integer = 600
    Sub AnimateObject(ByVal ObjectToAnimate As Object, ByVal showObject As Boolean, ByVal IsControl As Boolean, ByVal AnimateUpwards As Boolean)
        If showObject Then
            If IsControl Then
                If AnimateUpwards Then
                    ' ObjectToAnimate.Top = Me.Height
                    ' Transition.run(ObjectToAnimate, "Top", 654, (New TransitionType_EaseInEaseOut(animationSpeed)))
                    Dim t As New Transition(New TransitionType_EaseInEaseOut(animationSpeed)) : AddHandler t.TransitionCompletedEvent, AddressOf TransitionCompleted
                    t.add(ObjectToAnimate, "Height", 677) : t.run()
                Else
                    ' ObjectToAnimate.Left = Me.Width
                    Transition.run(ObjectToAnimate, "Left", 17, (New TransitionType_EaseInEaseOut(animationSpeed)))
                End If
            Else
                '  Me.Width = 560
                Dim t As New Transition(New TransitionType_EaseInEaseOut(animationSpeed)) : AddHandler t.TransitionCompletedEvent, AddressOf TransitionCompleted
                t.add(ObjectToAnimate, "Width", 903) : t.run()
                ' Transition.run(ObjectToAnimate, "Width", 903, (New TransitionType_EaseInEaseOut(animationSpeed)))
            End If
        Else
            If IsControl Then
                If AnimateUpwards Then
                    '  Transition.run(ObjectToAnimate, "Top", 463, (New TransitionType_EaseInEaseOut(animationSpeed)))
                    Dim t As New Transition(New TransitionType_EaseInEaseOut(animationSpeed)) : AddHandler t.TransitionCompletedEvent, AddressOf TransitionCompleted
                    t.add(ObjectToAnimate, "Height", 463) : t.run()
                Else
                    Transition.run(ObjectToAnimate, "Left", Me.Width, (New TransitionType_EaseInEaseOut(animationSpeed)))
                End If
            Else
                Dim t As New Transition(New TransitionType_EaseInEaseOut(animationSpeed)) : AddHandler t.TransitionCompletedEvent, AddressOf TransitionCompleted
                t.add(ObjectToAnimate, "Width", 560) : t.run()

            End If
        End If
    End Sub
    Public Sub TransitionCompleted()
        but_Expand.Enabled = True
        but_taller.Enabled = True
    End Sub
    Private Sub but_Expand_Click(sender As Object, e As EventArgs) Handles but_Expand.Click
        but_Expand.Enabled = False
        Select Case Me.Width
            Case Is >= 903
                AnimateObject(Me, False, False, False)
                but_Expand.Text = "⯈"
            Case Is < 903
                AnimateObject(Me, True, False, False)
                but_Expand.Text = "⯇"
        End Select
    End Sub

    Private Sub but_taller_Click(sender As Object, e As EventArgs) Handles but_taller.Click
        but_taller.Enabled = False
        Select Case Me.Height
            Case Is >= 677
                AnimateObject(Me, False, True, True)
                but_taller.Text = "⯆"
            Case Is < 677
                AnimateObject(Me, True, True, True)
                but_taller.Text = "⯅"
        End Select
    End Sub


